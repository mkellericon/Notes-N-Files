class Book

    attr_accessor :title, :author, :pages

    def initialize(title, author, pages)
        @title = title
        @author = author
        @pages = pages
    end
end

book1 = Book.new("Harry Potter", "JK Rolling", 400)

book2 = Book.new("Lord of The Rings", "Tolkein", 500)

puts book2.title

